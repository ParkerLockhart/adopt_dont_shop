<%= form_with url: url, method: method, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :boarding_services%>
  <%= f.check_box :boarding_services %><br>

  <%= f.label :max_patient_capacity %>
  <%= f.number_field :max_patient_capacity %><br>

  <%= f.submit %>
<% end %>
